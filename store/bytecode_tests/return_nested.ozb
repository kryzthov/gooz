Expected = 'xsx'

Sub = proc(
  nlocals: 1
  bytecode: segment(
    l0 := 's'
    call_native(name:print params:l*)
    return()
    l0 := 't'
    call_native(name:print params:l*)
  )
)

Main = proc(
  nlocals: 1
  bytecode: segment(
    l0 := 'x'
    call_native(name:print params:l*)
    call(proc:Sub params:p*)
    call_native(name:print params:l*)
  )
)
